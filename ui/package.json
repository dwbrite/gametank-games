{
  "name": "ui",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "build:webpack": "webpack --mode production",
    "build:sass": "sass css/styles.scss ../target/ui/styles.css",
    "build:public": "npx copyfiles --up 1 \"public/**/*\" ../target/ui",
    "watch:webpack": "webpack --watch --mode development",
    "watch:sass": "sass --watch css/styles.scss:../target/ui/styles.css",
    "watch:public": "cpx \"public/**/*\" ../target/ui --watch",
    "watch": "concurrently \"npm run watch:webpack\" \"npm run watch:sass\" \"npm run watch:public\""
  },

  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^9.2.1",
    "browserify": "^17.0.1",
    "coffee-loader": "^5.0.0",
    "coffeeify": "^3.0.1",
    "coffeescript": "^2.7.0",
    "concurrently": "^9.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "cpx": "^1.5.0",
    "css-loader": "^7.1.2",
    "sass": "^1.82.0",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "watchify": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "mithril": "^2.2.12",
    "path-browserify": "^1.0.1",
    "vm-browserify": "^1.1.2",
    "keycloak-js": "22.0.5"
  }
}
